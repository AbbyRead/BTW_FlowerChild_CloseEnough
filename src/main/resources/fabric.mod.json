{
  "schemaVersion": 1,
  "id": "fcce",
  "version": "${version}",

  "name": "Better Than Wolves: FlowerChild (Close Enough)",
  "description": "This is an attempt to port the original Better Than Wolves releases to community-supported versions of Minecraft, using good coding practices hopefully.",
  "authors": [
    "Abigail Read"
  ],
  "contact": {
    "homepage": "https://github.com/AbbyRead/BTW_FlowerChild_CloseEnough",
    "sources": "https://github.com/AbbyRead/BTW_FlowerChild_CloseEnough",
    "issues": "https://github.com/AbbyRead/BTW_FlowerChild_CloseEnough/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/fcce/BTW_Icon_Circle_512x512.png",

  "environment": "*",

  "entrypoints": {
    "stationapi:event_bus": [
      "ar.fcce.FCCE",
      "ar.fcce.FCCEBlocks",
      "ar.fcce.FCCERecipes",
      "ar.fcce.FCCEFireBurnable"
    ],
    "stationapi:event_bus_client": [
      "ar.fcce.FCCETextures"
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "fcce.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}